# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  Extensions_config = debug

else ifeq ($(config),release)
  Extensions_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := Extensions

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

Extensions:
ifneq (,$(Extensions_config))
	@echo "==== Building Extensions ($(Extensions_config)) ===="
	@${MAKE} --no-print-directory -C . -f Extensions.make config=$(Extensions_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f Extensions.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   Extensions"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"